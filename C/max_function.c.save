#include<stdio.h>
int max(int a,int b,int c){
    int result;
    if((a > b && a < c) || (a > c && a < b ) {
        result = a;
    }
    else if((b > c && b < a) ||(b < c && b > a ) ) {
        result = b;
    }
    else if((c > a && c < b) || (c < a && c )) {
        result = c;
    }
    return result;
}
int main()
{
    int a=10,b=5,c=4,result;
    result = max(a,b,c);
    printf("%d",result);
    }
