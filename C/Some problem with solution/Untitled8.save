//27.Write a C program to input any character and check whether it is alphabet, digit or
special character.

#include <stdio.h>
int main()
{
    char input;
    scanf("%c",&input);
    if((input >= 'a' && input <= 'z') || (input >= 'A' && input <= 'Z')){
        printf("Alphabet.\n");
    }
    else{
        printf("Not alphabet.\n");
    }
    return 0;

}
